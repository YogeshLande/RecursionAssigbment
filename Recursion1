//Write a Program to print the numbers between 1 to 10


public class Recursion1 {

    void printnum(int num){
        if(num==0) {
            return;
        }
        printnum(num-1);

        System.out.println(num);

    }
    public static void main(String[]args){
        Recursion1 obj = new Recursion1();
        
        obj.printnum(10);
    }
}
